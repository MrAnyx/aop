{
   "editor.tabSize": 3,
   "[typescript, javascript]": {
      "editor.formatOnSave": false
   },
   "[python]": {
      "editor.formatOnSave": true
   },
   "editor.codeActionsOnSave": {
      "source.fixAll": true
   },
   "prettier.printWidth": 999999,
   "python.formatting.provider": "black"
}